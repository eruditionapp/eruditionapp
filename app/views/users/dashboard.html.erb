<div class="page page-dashboard page-dashboard--light">
  <div class="wrapper wrapper--light">
    <div class="container">
      <div class="pagehead">
        <div class="row">
          <div class="col-sm-6">
            <ol class="breadcrumb">
              <li class="active">Dashboard</li>
            </ol>
          </div>
          <div class="col-sm-6">
            <div class="btn-toolbar pull-right">
              <!-- <div class="btn-group">
                <%= link_to '##', '#', class: 'btn btn-default' %>
              </div> -->
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        <div class="grid">
          <% @decks.each do |deck| %>
            <%= render 'dashboard_deck', deck: deck %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
